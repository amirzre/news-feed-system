# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=news_feed
DB_SSL_MODE=disable

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# News API Configuration
NEWS_API_KEY=your_news_api_key_here
NEWS_API_BASE_URL=https://newsapi.org/v2

# Server Configuration
SERVER_PORT=8080
SERVER_HOST=localhost

# Application Configuration
APP_ENV=development
LOG_LEVEL=debug

# Cache Configuration
CACHE_TTL=3600

# CORS Configuration
# Allow all origins use * for development, Multiple domains example:
# CORS_ALLOW_ORIGINS=http://localhost:3000,http://localhost:3001,https://myapp.com,https://www.myapp.com
CORS_ALLOW_ORIGINS=*
CORS_ALLOW_METHODS=GET,POST,PUT,DELETE,PATCH,OPTIONS
CORS_ALLOW_HEADERS=Origin,Content-Type,Accept,Authorization,X-Requested-With
CORS_EXPOSE_HEADERS=
CORS_ALLOW_CREDENTIALS=false
CORS_MAX_AGE=86400
